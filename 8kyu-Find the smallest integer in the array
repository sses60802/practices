public class SmallestIntegerFinder {
    public static int findSmallestInt(int[] args) {
      if (args.length == 0||args == null || args.length < 1)
      return 0;
      int x = args[0];
      for(int j = 0 ;j < args.length ; j++){  
          if(args[j] < x)
          x = args[j];
      }
    return x;
    }
}
