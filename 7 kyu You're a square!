public class Square {    
    public static boolean isSquare(int n) {        
        if(n<0){
        return false;
        }
        if(n == 0||n == 1){
        return true;
        }
        if(n>1){
          for(int i = 2;i < n; i++){
            if((i*i) == n)
              return true;
            if((i*i)>n)
              return false;
          }
        }
    return true;
    }
}
