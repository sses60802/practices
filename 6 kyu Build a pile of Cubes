public class ASum {
	
	public static long findNb(long m) {
		// your code
      long count = 0;
      for(long i = 1; i < m; i++){
        count =  count + (long)Math.pow(i,3);
        if (count == m)
          return i;
        else if(count > m)
          return -1; 
      }
      return 0;
	}	  

}
