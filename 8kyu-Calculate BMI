public class Calculate {
  public static String bmi(double weight, double height) {
    String bmi1 = "";
    double bmi = weight/(height*height);
    if(bmi <= 18.5)  
        bmi1 = "Underweight";
    else if (bmi <= 25.0)
        bmi1 = "Normal";
    else if (bmi <= 30.0)
        bmi1 = "Overweight";
    else if (bmi > 30)
        bmi1 = "Obese";
    return bmi1;
  }
}
